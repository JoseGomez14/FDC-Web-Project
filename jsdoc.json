{
    "plugins": [],
    "source": {
        "include": ["src"],
        "includePattern": ".+\\.js(doc|x)?$",
        "exludePattern": "(node_modules|docs)"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "recurse": "true",
        "destination": "./docs"
    }
}